{
    "name": "askaoru/routable",
    "description": "Easily create routable models in laravel 5 projects",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "askaoru",
            "email": "askaoru04@gmail.com",
            "homepage": "https://github.com/askaoru",
            "role": "Developer"
        }
    ],
    "minimum-stability": "dev",
    "require": {
        "php": ">=5.4.0",
        "laravel/framework": "~5.0|~5.1|~5.2|~5.3|~5.4"
    },
    "require-dev": {
        "phpunit/phpunit": "4.*|5.*|6.*",
        "orchestra/testbench": "~3.0",
        "adamwathan/faktory": "0.3.*"
    },
    "autoload": {
        "psr-4": {
            "Askaoru\\Routable\\": "src"
        }
    },
    "extra":{
        "laravel":{
            "providers":[
                "Askaoru\\Routable\\RoutableServiceProvider"
            ]
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Askaoru\\Routable\\Tests\\": "tests"
        }
    }
}
